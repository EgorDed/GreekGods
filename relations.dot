// === Поколение Олимпийцев ===
Cronus -> { Zeus Poseidon Hades Hera Demeter Hestia };

// === Дети Зевса ===
Zeus -> { Athena Apollo Artemis Ares Hermes Dionysus };

// === Герои ===
Zeus -> Heracles;
Zeus -> Perseus;
Zeus, Nemesis -> Helen;

// === Монстры ===
Tartarus, Nemesis -> Telkhines
